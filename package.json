{
  "name": "turtle-program",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "author": {
    "name": "MS Team",
    "email": "changeme@gmail.com",
    "url": "https://your-website.com"
  },
  "build": {
    "appId": "com.ms-team",
    "productName": "Turtle Program",
    "copyright": "Copyright Â© 2019 MS Team",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "cross-env": "^6.0.3",
    "electron-is-dev": "^1.1.0",
    "esprima": "^4.0.1",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-ace": "^8.0.0",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "web-start": "parcel index.html",
    "web-build": "parcel build --public-url '.' index.html",
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test --watchAll=false",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "eject": "react-scripts eject",
    "dist-mac": "electron-builder -m --publish=never",
    "dist-windows": "electron-builder -w --publish=never",
    "pretty": "prettier --config \"./.prettierrc\" --write \"src/**/*.{js,css,less,scss,ts,tsx,json}\"",
    "prettier-check": "prettier --check --config \"./.prettierrc\" \"src/**/*.{js,css,less,scss,ts,tsx,json}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@rescripts/cli": "^0.0.12",
    "@rescripts/rescript-env": "^0.0.10",
    "babel-plugin-transform-runtime": "^7.0.0-beta.3",
    "concurrently": "^5.0.0",
    "cypress": "^3.8.2",
    "electron": "^11.5.0",
    "electron-builder": "^21.2.0",
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.6.4",
    "wait-on": "^3.3.0"
  }
}
